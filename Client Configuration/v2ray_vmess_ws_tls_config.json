{
  "log": {},
  "inbounds": [
    {
      "port": "1080",
      "protocol": "socks",
      "settings": {
        "udp": true
      }
    },
    {
      "port": "1081",
      "protocol": "http",
      "settings": {}
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "domain": ["geosite:cn"], //中国大陆主流网站的域名
        "outboundTag": "direct" //与下 outbounds 中 tag 对应
      },
      {
        "type": "field",
        "ip": [
          "geoip:cn", //中国大陆的 IP
          "geoip:private" //私有地址 IP，如路由器等。
        ],
        "outboundTag": "direct" //与下 outbounds 中 tag 对应
      }
    ]
  },
  "outbounds": [
    {
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "cdn.example.com", //修改为使用本应用的域名
            "port": 443,
            "users": [
              {
                "id": "21376258-dd56-11e9-aa37-5600024c1d6a", //修改为服务端配置的 UUID
                "security": "auto"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "tlsSettings": {
          "alpn": ["http/1.1"],
          "fingerprint": "chrome" //模拟 TLS 指纹，可任一 chrome、firefox、safari、ios、edge、qq、random、randomized 选项配置。V2Ray（v4 版） 不支持，此项配置必须删除。
        },
        "wsSettings": {
          "path": "/HALdGZ9k" //修改为服务端配置的路径。大于 Xray-core v1.8.9 版本，路径后加上 ?ed=2560 即可开启 WebSocket 0-RTT。
        }
      },
      "mux": {
        "enabled": true, //启用多路复用
        "concurrency": 4 //此数据根据网络情况调整。表示 4 条传输进程复合一个链路发出。
      }
    },
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {}
    }
  ]
}
