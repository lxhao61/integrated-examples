  "inbounds": [
    {
      "listen": "@vless.sock", /VLESS+Vision+TLS 监听进程
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "048e0bf2-dd56-11e9-aa37-5600024c1d6a", //修改为自己的 UUID
            "flow": "xtls-rprx-vision", //启用 XTLS Vision
            "email": "5443@gmail.com"
          }
        ],
        "decryption": "none",
        "fallbacks": [
          {
            "dest": "@h1h2c.sock", //h2 回落与 http/1.1 回落共用进程
            "xver": 2 //开启 PROXY protocol 发送，发送真实来源 IP 和端口给 Caddy。 1 或 2 表示 PROXY protocol 版本。
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "certificates": [
            {
              "ocspStapling": 3600,
              "certificateFile": "/home/tls/certificates/acme-v02.api.letsencrypt.org-directory/zv.xx.yy/zv.xx.yy.crt", //换成自己的证书，绝对路径。
