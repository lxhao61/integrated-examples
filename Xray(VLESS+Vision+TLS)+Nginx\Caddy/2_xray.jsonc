{
  "log": {
    "loglevel": "warning",
    "error": "/var/log/xray/error.log",
    "access": "/var/log/xray/access.log"
  },
  "inbounds": [
    {
      "port": 443, //监听端口
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "048e0bf2-dd56-11e9-aa37-5600024c1d6a", //修改为自己的 UUID
            "flow": "xtls-rprx-vision", //启用 XTLS Vision
            "email": "443@gmail.com"
          }
        ],
        "decryption": "none",
        "fallbacks": [
          {
            "dest": "/dev/shm/uds88.sock", //回落给 H2C server 及 HTTP/1.1 server 监听进程
            "xver": 1 //开启 PROXY protocol 发送，发送真实来源 IP 和端口给 Nginx。 1 或 2 表示 PROXY protocol 版本。
          } //对应 Nginx 版本不小于 v1.25.1 配置，否则此部分必须删除。
          {
            "alpn": "h2",
            "dest": "/dev/shm/uds82.sock", //回落给 H2C server 监听进程
            "xver": 1 //开启 PROXY protocol 发送，发送真实来源 IP 和端口给 Nginx。 1 或 2 表示 PROXY protocol 版本。
          }, //对应 Nginx 版本小于 v1.25.1 配置，否则此部分必须删除。
          {
            "dest": "/dev/shm/uds81.sock", //回落给 HTTP/1.1 server 监听进程
            "xver": 1 //开启 PROXY protocol 发送，发送真实来源 IP 和端口给 Nginx。 1 或 2 表示 PROXY protocol 版本。
          } //对应 Nginx 版本小于 v1.25.1 配置，否则此部分必须删除。
          {
            "dest": "@uds88.sock", //回落给 H2C server 及 HTTP/1.1 server 监听进程
            "xver": 1 //开启 PROXY protocol 发送，发送真实来源 IP 和端口给 Caddy。 1 或 2 表示 PROXY protocol 版本。
          } //对应 Caddy 配置，否则此部分必须删除。
        ]
      },
      "streamSettings": {
        "network": "raw",
        "security": "tls",
        "tlsSettings": {
          "rejectUnknownSni": true, //限定域名连接（包括禁止以 IP 方式访问网站）
          "certificates": [
            {
              "ocspStapling": 3600,
              "certificateFile": "/home/tls/h2t.example.com/h2t.example.com.crt", //换成自己的证书，绝对路径。
              "keyFile": "/home/tls/h2t.example.com/h2t.example.com.key" //换成自己的密钥，绝对路径。
            }
          ],
          "minVersion": "1.2"
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls",
          "quic"
        ]
      }
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "protocol": [
          "bittorrent"
        ],
        "outboundTag": "block"
      }
    ]
  },
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag": "block",
      "protocol": "blackhole",
      "settings": {}
    }
  ]
}
